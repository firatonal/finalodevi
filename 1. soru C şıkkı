import pandas as pd

sol_durum = {
    'Tsol': [1, 5, 5, 2, 5, 4, 6, 5],
    'Psol': [0.09, 0.45, 0.45, 0.18, 0.45, 0.36, 0.55, 0.45]
}

sag_durum = {
    'Tsag': [3, 3, 2, 3, 2, 2, 5],
    'Psag': [0.67, 0.67, 0.33, 0.67, 0.33, 0.33, 0.56]
}

sol_df = pd.DataFrame(sol_durum)
sag_df = pd.DataFrame(sag_durum)

print("Sol Durum:")
print(sol_df)
print("\nSaÄŸ Durum:")
print(sag_df)
