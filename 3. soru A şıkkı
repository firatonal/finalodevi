import pandas as pd

data = {
    'x': [1, 2, 2, 3, 4, 5, 6, 8, 10, 11],
    'y': [3, 5, 3, 9, 7, 2, 8, 6, 6, 1],
    'z': ['negatif', 'pozitif', 'pozitif', 'negatif', 'negatif', 'pozitif', 'pozitif', 'negatif', 'negatif', 'negatif']
}

df = pd.DataFrame(data)
print(df.to_string(index=False))

x_koordinatlari = df['x']
y_koordinatlari = df['y']

print("X Koordinatları:", x_koordinatlari.tolist())
print("Y Koordinatları:", y_koordinatlari.tolist())
